      <em:iconURL>chrome://reduceiframe/skin/iframe.ico</em:iconURL>      
      <em:icon64URL>chrome://reduceiframe/skin/iframe.png</em:icon64URL>

      <em:optionsURL>chrome://reduceiframe/content/options.xul</em:optionsURL>
      <em:optionsType>2</em:optionsType>

